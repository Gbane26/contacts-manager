{{ form_start(form, { 'attr': { 'enctype': 'multipart/form-data' } }) }}
  
  <div class="row mb-4">
    <div class="col">
      {{ form_row(form.name) }}
    </div>
    <div class="col">
      {{ form_row(form.description) }}
    </div>
  </div>
  <button data-mdb-ripple-init type="submit" class="btn btn-primary btn-block mb-4">Save</button>

{{ form_end(form) }}
